{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./src/components",
          "config": "./src/config",
          "graphQL": "./src/graphql",
          "hooks": "./src/hooks",
          "libs": "./src/libs",
          "assets": "./src/assets"
        }
      }
    ]
  ]
}

{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "components": "./src/components",
          "config": "./src/config",
          "graphql": "./src/graphql",
          "hooks": "./src/hooks",
          "libs": "./src/libs",
          "assets": "./src/assets",
          "services": "./src/services",
          "utils": "./src/utils",
          "providers": "./src/providers"
        }
      }
    ]
  ]
}
